    "jasmine-spec-reporter": "4.2.1",
    "json-server": "0.14.0",
    "karma": "2.0.4",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-phantomjs-launcher": "1.0.4",
    "less-loader": "4.0.5",
    "minimist": "1.2.0",
    "mocha": "5.2.0",
    "mochawesome": "3.0.3",
    "nodemon": "1.17.5",
    "nsp": "3.2.1",
    "nyc": "13.0.1",
    "postcss-import": "11.0.0",
    "postcss-loader": "2.0.10",
    "postcss-url": "7.1.2",
    "protractor": "5.4.1",
    "protractor-cucumber-framework": "5.0.0",
    "protractor-multiple-cucumber-html-reporter-plugin": "1.7.1",
    "proxyquire": "2.0.1",
    "raw-loader": "0.5.1",
    "sass-loader": "6.0.6",
    "should": "13.2.3",
    "socks-proxy-agent": "3.0.1",
    "sonar-scanner": "3.0.3",
    "style-loader": "0.19.1",
    "stylus-loader": "3.0.1",
    "supertest": "3.1.0",
    "ts-node": "4.1.0",
    "tslint": "5.10.0",
    "typescript": "2.7.2",
    "uglifyjs-webpack-plugin": "1.1.8",
    "url-loader": "0.6.2"
  },
  "snyk": true,
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "include": [
      "api/**/**"
    ],
    "exclude": [
      "**/*.spec.ts",
      "**/*.spec.js",
      "test/**",
      "config/**",
      "api/**/templates/**",
      "src/public/**",
      "src/environments/**"
    ],
    "reporter": [
      "lcov",
      "html",
      "text-summary"
    ],
    "require": [],
    "cache": true,
    "all": true,
    "report-dir": "./reports/tests/coverage/node"
  },
  "eslintConfig": {
    "extends": "@hmcts",
    "env": {
      "es6": true,
      "node": true,
      "jasmine": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "indent": [
        "error",
        4
      ],
      "max-len": [
        2,
        120
      ]
    }
  },
  "eslintIgnore": [
    "**/node_modules/**",
    "src/public/**"
  ]
}
