{
  "name": "jui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "nuke": "rm -rf dist",
    "build": "tsc && ng build --prod && ng run jui:server",
    "start": "cd dist/jui-backend && node server.js",
    "start-dev-proxy": "ng serve --proxy-config proxy.config.json",
    "start-dev-node": "cd dist/jui-backend && export JUI_ENV=local; node dev-server.js",
    "start-json-server": "export JUI_ENV=mock; node ./support/mock-server/mockserver.js",
    "lint": "npm run lint:node || npm run lint:ng || true",
    "lint:node": "eslint -f node_modules/eslint-html-reporter/reporter.js -o ./reports/lint/node/index.html .",
    "lint:ng": "ng lint jui",
    "lint:fix": "npm run lint:fix:node || npm run lint:fix:ng || true",
    "lint:fix:node": "eslint . --fix",
    "lint:fix:ng": "ng lint jui --fix",
    "test": "npm run test:node && npm run test:ng",
    "test:node": "echo 'not implemented'",
    "test:ng": "ng test",
    "test:api": "node ./test/integration/tests/test.js",
    "test:nsp": "nsp check",
    "test:coverage": "echo 'not implemented'",
    "test:coverage:merge": "./node_modules/.bin/lcov-result-merger './reports/tests/coverage/*/lcov.info' > ./reports/tests/coverage/lcov.info",
    "test:coverage:node": "nyc jasmine --config=test/unit/node/jasmine.json",
    "test:coverage:ng": "ng test --code-coverage",
    "test:a11y": "echo 'not implemented'",
    "test:smoke": "webdriver-manager update --versions.chrome 2.40 && protractor ./test/e2e/config/smoke.conf.js",
    "test:smoke:local": "webdriver-manager update --versions.chrome 2.40 && protractor ./test/e2e/config/smoke.conf.js --local",
    "test:functional": "webdriver-manager update --versions.chrome 2.40 && protractor ./test/e2e/config/conf.js",
    "test:functional:local": "webdriver-manager update --versions.chrome 2.40 && protractor ./test/e2e/config/conf.js --local",
    "test:crossbrowser": "protractor ./test/e2e/config/crossbrowser.conf.js",
    "test:fullfunctional": "echo 'not implemented'",
    "test:mutation": "./bin/run-mutation-tests.sh",
    "test:apiGateway": "echo 'not implemented'",
    "sonar-scan": "node_modules/sonar-scanner/bin/sonar-scanner",
    "test:codacy-upload": "cat ./reports/tests/coverage/lcov.info | codacy-coverage",
    "test:codecov-upload": "codecov -f ./reports/tests/coverage/lcov.info",
    "snyk": "snyk test",
    "snyk-protect": "snyk protect",
    "doc:ng": "compodoc -p src/tsconfig.app.json -s"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:ng",
      "pre-push": "npm run lint:ng && npm run test && npm run test:nsp "
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.0",
    "@angular/cdk": "6.1.0",
    "@angular/cli": "6.1.0",
    "@angular/common": "6.1.0",
    "@angular/compiler": "6.1.0",
    "@angular/core": "6.1.0",
    "@angular/forms": "6.1.0",
    "@angular/http": "6.1.0",
    "@angular/material": "6.1.0",
    "@angular/platform-browser": "6.1.0",
    "@angular/platform-browser-dynamic": "6.1.0",
    "@angular/platform-server": "6.1.0",
    "@angular/router": "6.1.0",
    "@hmcts/frontend": "0.0.14-alpha",
    "@hmcts/info-provider": "1.0.0",
    "@hmcts/nodejs-healthcheck": "1.4.6",
    "@hmcts/nodejs-logging": "3.0.0",
    "@nguniversal/express-engine": "6.0.0",
